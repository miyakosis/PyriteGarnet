/*
  break, continueæ–‡
*/
class	PyrT13
{
	static main(var args : [str])()
	{
		var phase = 1;
		for (var i = 0; i <= 6; i += 1)
		{
			
			if (i == 1)
			{
				continue;
			}
			if (i == 3)
			{
				break;
			}
			
			System.out.println(i);
		}

		phase = 2;
		var i = 0;
		while(true)
		{
			if (i == 1)
			{
				i += 1;
				continue;
			}
			if (i == 3)
			{
				break;
			}
			
			System.out.println(i);
			i += 1;
		}

		phase = 3;
		i = 0;
		for(;;)
		{
			if (i == 1)
			{
				i += 1;
				continue;
			}
			if (i == 3)
			{
				break;
			}
			
			System.out.println(i);
			i += 1;
		}
	}
}
