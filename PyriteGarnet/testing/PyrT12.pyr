/*
  switch æ–‡
*/
class	PyrT12
{
	static main(var args : [str])()
	{
		for (var i = 0; i <= 6; i += 1)
		{
			var s : str = "";
			
			switch (i)
			{
			case 0:
				s += "0";
			case 1:
				s += "1";
			case 2:
				s += "2";
				fallthrough;
			case 3:
				s += "3";
			case 4:
				s += "4";
				fallthrough;
			default:
				s += "d";
			}
			System.out.println(s);
		}
	}
}
